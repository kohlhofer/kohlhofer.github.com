$iconWhiteSpritePath: "../images/glyphicons-halflings-white.png";

@import "bootstrap-sass/lib/bootstrap";
@import "fonts.css";

/* Put your CSS here */
html, body {

}


body {
  background-color:#4A6072;
  font-size:14px;
}

a {
  cursor:pointer;
}




.home-page {
  background-color:#7C97AC;
}

// Navigation


.navbar {
  box-shadow: 0px 1px 1px rgba(0,0,0,0.1);
  border-width:0px;
  background-color: #FFF;
  padding:1em 1.5em;
  font-size: 1.2em;
  a {
    &:hover {
      text-decoration:none;
    }
  }
}

.navbar-toggle {
  border-width:0px;
  border-radius:0px;
  &:hover {
    background-color:#FFF;
  }
}

.top-navigation-link {
  font-size:0.9em;
  font-family: 'Lato', sans-serif;
  font-weight:400;
  text-transform:uppercase;
  letter-spacing:1px;
}

.top-navigation-link-social {
  @extend .top-navigation-link;
  transition:all cubic-bezier(0.250, 0.460, 0.450, 0.940) 0.3s;
  opacity:0.4;
  display:none !important;
  @media (min-width: $screen-sm) {
    display:inline-block !important;
  }
  &:hover {
    transform: translate(0px, -2px);
    opacity:1;
  }
}



// Header


.header-section {
  margin-bottom:1.5em;
  position:relative;
  text-align:center;
  //background: linear-gradient(to bottom, #456d8c 30%,#7c97ac 100%);
}

.controls {
  width:100%;
  position:absolute;
  top:50%;
  font-size:1.5em;
  a {
    color:rgba(255,255,255,0.5);
    padding:0.5em 1em;
    &:hover {
      color:rgba(255,255,255,0.8);
    }
  }
  @media (max-width: $screen-md) {
   top:auto;
   bottom:1em;
  }
}

.coffee-mug {
  margin-top:50px;
  transform:rotate(120deg);
}

.header-title {
  font-size: 2.2em;
  margin:0;
  font-family: 'Quicksand', sans-serif;
  font-weight:400;
  padding:7em 0 0 0;
  color:#d3e777;
}

.header-sub-title {
  margin:0.5em 0 0 0;
  font-size: 1.5em;
  color:#BCD;
  font-family: 'Lato', sans-serif;
  font-weight:300;
  padding-bottom:10em;;
}

.header-slide {
  overflow:hidden;
}




// projects


.project-section {
  padding:2em $grid-gutter-width 4em;
  @media (min-width: $screen-md) {
   text-align:left;
  }
}

.project-section-title {
  font-size: 1.2em;
  padding:0;
  margin:0 0 1em 0;
  color:#FFF;
  font-family: 'Lato', sans-serif;
  font-weight:300;
  text-transform:uppercase;
  letter-spacing:1px;
}

.project-section-header {
  margin-bottom: 1.5em;
}


.project-section-text {
  font-size: 1.2em;
  margin-bottom:1.5em;
  color:rgba(255,255,255,0.7);
  line-height:1.6em;
  font-family: 'Lato', sans-serif;
  font-weight:300;
  a {
    font-weight:400;
    color:rgba(255,255,255,0.8);
  }
}

.project-section-filter {
  margin-bottom: 2em;
  position:relative;
  .clear-input {
    position:absolute;
    font-size:1.2em;
    right:1.5em;
    color:#D00;
    top:0.3em;
  }
}

.project-section-filter-input {
  border-width:0px;
  border-radius:0px;
  font-size:1.1em;
  display:block;
  width:100%;
  padding:0.3em 0.5em;
  padding-right:2em;
  font-family: 'Lato', sans-serif;
  font-weight:400;
  background-color:#EEE;
}


.project-list-image {
  transition:all cubic-bezier(0.250, 0.460, 0.450, 0.940) 0.3s;
}

.project-section-project-tags {
  transition:all cubic-bezier(0.250, 0.460, 0.450, 0.940) 0.5s;
  margin-top:-1em;
  opacity:0;
  color:rgba(0,0,0,0.7);
  font-family: 'Lato', sans-serif;
  font-weight:400;
  text-transform:uppercase;
  font-size:0.8em;
  span {
    margin:0 0.2em;
  }
}

.project-section-project-title {
  color:#333;
  text-align:center;
  font-size:1.1em;
  margin:0.8em 0 0 0;
  font-family: 'Lato', sans-serif;
  font-weight:400;
  transition:all cubic-bezier(0.250, 0.460, 0.450, 0.940) 0.2s;
}


.project-section-project-link {
  display:block;
  padding:10%;
  text-align:center;
  margin-bottom:$grid-gutter-width;
  border-radius:3px;
  &:hover {
    text-decoration:none;
    .project-list-image {
      transform:translate(0px,-1em) scale(0.9);
    }
    .project-section-project-tags {
      opacity:1;
    }
    .project-section-project-title {
      transform:translate(0px,-1em);
    }
  }
}







// about section 

.about-section {
  padding: 4em 0 4em 0;
}

.about-section-text {
  font-size: 1.2em;
  margin:0;
  color:rgba(0,0,0,0.6);
  line-height:1.6em;
  font-family: 'Lato', sans-serif;
  font-weight:400;
  a {
    color:rgba(0,0,0,0.6);
    border-bottom:1px solid rgba(0,0,0,0.3);
    &:hover {
      text-decoration:none;
      border-bottom:1px solid rgba(0,0,0,0.6);
    }
  }
}




// CV section



.cv-section {
  background-color:#4A6072;
}

.cv-section-header {
  text-align:center;
  padding:4em 0;
}

.cv-reverse-link {
  background-color:#7C97AC;
  display:inline-block;
  font-size:0.9em;
  color:#4A6072;
  padding:0.5em 1em;
  border-radius:2em;
  text-transform:uppercase;
  margin:1em 0;
  font-family: 'Lato', sans-serif;
  font-weight:400;
  &:hover {
    text-decoration:none;
  }
}

.cv-page-title {
  font-size: 1.2em;
  margin:0;
  color:#7C97AC;
  font-family: 'Lato', sans-serif;
  font-weight:300;
  text-transform:uppercase;
  letter-spacing:1px;
}

.cv-page-year {
  margin-bottom: 2em;
  
}




.cv-page-year-label {
  text-align:right;
  font-size: 1.2em;
  line-height:1.6em;
  font-family: 'Lato', sans-serif;
  font-weight:700;
  color:#7C97AC;
}

.cv-entry-event {
  margin-bottom:$grid-gutter-width;
  font-size: 1.2em;
  line-height:1.6em;
  font-family: 'Lato', sans-serif;
  font-weight:300;
  color:rgba(255,255,255,0.6);
  a {
    color:rgba(255,255,255,0.8);
    font-weight:400;
  }
}






// connect section

.connect-section {
  padding:3em 0 6em;
}


.social-list {
  text-align:center;
}


.social-list-entry {
  display:inline-block;
  margin-left: $grid-gutter-width/3;
  margin-right: $grid-gutter-width/3;
  margin-bottom: $grid-gutter-width/1.5;
}

.social-list-entry-link {
  background-color:#FFF;
  color:#999;
  display:block;
  font-size:1.7em;
  height:2 * $grid-gutter-width;
  line-height:2.1 * $grid-gutter-width;
  width:2 * $grid-gutter-width;
  border-radius:5em;
  transition:all cubic-bezier(0.250, 0.460, 0.450, 0.940) 0.3s;
  &:hover {
    transform: scale(1.2) rotate(360deg);
  }
}












// porject detail view

.project-page-header {
  box-shadow: 0px 1px 1px rgba(0,0,0,0.2);
}

.project-page-header-label {
  display:inline-block;
  padding:4em 0em 2em 0em;
}


.project-page-header-title {
  color:rgba(0,0,0,0.6);
  font-family: 'Lato', sans-serif;
  font-weight:400;
  font-size:2em;
  line-height: 1.2em;
  margin: 0 0.5em 0 0;
  display:inline-block;
}

.project-page-header-sub-title {
  display:inline-block;
  margin: 1em 0 0 0;
  text-transform:uppercase;
  font-size:0.9em;
  color:rgba(0,0,0,0.4);
  font-family: 'Lato', sans-serif;
  font-weight:400;
  letter-spacing:1px;
  a {
    color:rgba(0,0,0,0.6);
  }
}

.close-project-page-link {

  font-size:1.4em;
  transition:all cubic-bezier(0.250, 0.460, 0.450, 0.940) 0.3s;
  &.icon-cross {
    background-color:rgba(0,0,0,0.7);
    height:2.5em;
    width:2.5em;
    line-height:2.6em;
    text-align:center;
    color:rgba(255,255,255,0.6);
  }
  &:hover.icon-cross {
    color:rgba(255,255,255,0.9);
    background-color:rgba(0,0,0,0.9);
    transform:rotate(90deg) scale(0.8);
  }
}






// ribbon on project list


.ribbon-wrapper {
  width: 90px;
  height: 90px;
  overflow: hidden;
  position: absolute;
  top: 0px;
  right: $grid-gutter-width/2;
  z-index:1;
}

.ribbon {
  font: bold 15px Sans-Serif;
  color: #333;
  text-align: center;
  transform:      rotate(45deg);
  position: relative;
  padding: 5px 0;
  left: -5px;
  top: 15px;
  width: 130px;
  background-color: #FFF;
  color: #000;
  &.new-ribbon {
    background-color:#BFDC7A;
    color:#6a6340;
  }
}















// project list animation

.animate-repeat.ng-enter,
.animate-repeat.ng-leave,
.animate-repeat.ng-move {
  transition:all linear 0.1s;
}
.animate-repeat.ng-enter {
  transform: rotateY(90deg);
}
.animate-repeat.ng-enter.ng-enter-active {
  transform: rotateY(0deg);
}
.animate-repeat.ng-leave {
  transform: rotateY(0deg);
}
.animate-repeat.ng-leave.ng-leave-active {
  transform: rotateY(90deg);
}
.animate-repeat.ng-move { }
.animate-repeat.ng-move.ng-move-active { }




